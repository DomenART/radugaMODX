<header class="header">
	<ul class="header-social">
		<li>
			<a href="{'site_vk' | url}" aria-label="Страница Вконтакте">
			<svg class="svg-vkontakte" width="26" height="26">
				<use xlink:href="assets/components/app/web/img/sprite.svg#vkontakte"/>
			</svg>
			</a>
		</li>
		<li>
			<a href="{'site_instagram' | url}" aria-label="Профиль в инстаграме">
			<svg class="svg-instagram" width="19" height="19">
				<use xlink:href="assets/components/app/web/img/sprite.svg#instagram"/>
			</svg>
			</a>
		</li>
		<li>
			<a href="{'site_youtube' | url}" aria-label="Канал на ютубе">
			<svg class="svg-youtube" width="20" height="20">
				<use xlink:href="assets/components/app/web/img/sprite.svg#youtube"/>
			</svg>
			</a>
		</li>
	</ul>
	<a href="mailto:{'site_email' | config}" class="header__email" title="Электронная почта">{'site_email' | config}</a>
	<a href="tel:{'site_tel' | config}" class="header__phone" title="Телефон">{'site_tel' | config}</a>
</header>
